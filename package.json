{
  "files": [
    "lib/**/*"
  ],
  "name": "test-scenario-runner",
  "version": "1.0.24",
  "description": "A test scenario runner",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/(*.js|*.ts)\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelstevick/test-scenario-runner.git"
  },
  "keywords": [
    "Testing"
  ],
  "author": "Joel Stevick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joelstevick/test-scenario-runner/issues"
  },
  "homepage": "https://github.com/joelstevick/test-scenario-runner#readme",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "colors": "^1.4.0"
  }
}
